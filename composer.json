{
  "name": "seothemes/genesis-config-exporter",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "seothemes",
      "email": "seothemeswp@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "brain/monkey": "^2.3",
    "phpunit/phpunit": "^8.3",
    "squizlabs/php_codesniffer": "^3.4",
    "wp-cli/wp-cli-bundle": "^2.3",
    "wp-coding-standards/wpcs": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "SeoThemes\\GenesisConfigExporter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SeoThemes\\GenesisConfigExporter\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --bootstrap=vendor/autoload.php tests/ --colors=always"
  }
}
